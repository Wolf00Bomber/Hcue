            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.appdest.hcue.FeedbackActivity"
            android:screenOrientation="landscape"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.appdest.hcue.ConfirmationFullViewActivity"
            android:screenOrientation="landscape"
            android:label="@string/app_name" >
